#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	   int sayi1,sayi2,toplam;
	    sayi1 = 6;
	    sayi2 = 6;
	    toplam = sayi1+sayi2;
	      if(toplam<12){
	      	printf("Toplam %d Bu Sayıdan Küçüktür",toplam);
		  }
		  else{
		  	printf("Toplam %d  Bu Sayilara Eşittir",toplam);
		  }
		  
	return 0;
}
